    "@skalenetwork/upgrade-tools": "1.0.0-develop.11",
    "@typechain/hardhat": "^4.0.0",
    "dotenv": "^16.0.0",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.5.3",
    "hardhat": "^2.8.3"
  },
