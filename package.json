    "solhint": "^3.2.1",
    "solidity-coverage": "^0.7.14",
    "solidity-docgen": "^0.5.7",
    "ts-generator": "^0.1.1",
    "ts-node": "9.0.0",
    "tslint": "^6.1.3",
