    "@types/chai-almost": "^1.0.1",
    "@types/chai-as-promised": "^7.1.3",
    "@types/elliptic": "^6.4.14",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "bignumber.js": "^9.0.2",
    "chai": "^4.3.6",
